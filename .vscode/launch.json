{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Debug Nest Framework",
      "runtimeExecutable": "yarn",
      "runtimeArgs": [
        "start:debug",
        "--",
        "--inspect-brk"
      ],
      "autoAttachChildProcesses": true,
      "restart": true,
      "sourceMaps": true,
      "stopOnEntry": false,
      "console": "integratedTerminal",
      "cwd": "${workspaceFolder}/api",
      "env": {
        "DEFAULT_ADMIN_EMAIL": "test@test.de",
        "JWT_SECRET": "secret",
        "SESSION_SECRET": "session-secret"
      }
    }
  ]
}